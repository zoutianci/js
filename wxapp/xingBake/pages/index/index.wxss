/**index.wxss**/
.xingBake {
  width: 750rpx;
  height: 100%;
  margin: 0;
  padding: 0;

}
.top {
  height: 500rpx;
  width: 100%;
  position: relative;
  /* background-color: red; */
}
.top image{
  width: 100%;
  height: 500rpx;
}
.md {
  
  width: 100%;
  height: 150rpx;
  /* align-items: center; */
  position: relative;
  /* background-color: purple; */
  /* text-align: center; */
  /* margin-top: -25rpx; */
}
.md .mdUrl {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -25%;
  margin-top: -50rpx;
  /* margin: 25rpx auto; */
  /* text-align: center; */
  width: 50%;
  height: 100rpx;
  font-size: 32rpx;
  border-radius: 50rpx;
  border: 1px solid #000;
  line-height: 100rpx;
  box-sizing: border-box;
}
.md_icon {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  margin: 35rpx 0 0 40rpx;
  /* margin-top: 10rpx; */
  margin-right:25rpx;
}
.md_content {
  position: absolute;
  margin-left: 100rpx;
}
.listTab {
  width: 100%;
  position: relative;
  margin: 0 20rpx;
}
.list_title {
  font-size: 30rpx;
}

.category-right{
  width:100%;
  height: 100%;
  overflow: hidden;
}
.cate-box{
  height: 100%;
  padding: 40rpx;
  display: block;
  box-sizing: border-box;
}
.cate-box .cate-banner image{
  display: block;
  width: 100%;
  height: 300rpx;
  /* border: 1px solid red;
  box-sizing: border-box; */
}
.cate-title{
  position: relative;
  height: 30rpx;
  line-height: 30rpx;
  padding: 30rpx 0 55rpx;
  text-align: center;
  color: #ab956d;
  font-size: 28rpx;
}
